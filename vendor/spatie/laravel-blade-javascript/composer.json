{
    "name": "spatie/laravel-blade-javascript",
    "description": "A Blade directive to export variables to JavaScript",
    "keywords": [
        "spatie",
        "laravel",
        "blade",
        "javascript",
        "directive",
        "laravel-blade-javascript"
    ],
    "homepage": "https://github.com/spatie/laravel-blade-javascript",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.22|^9.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\BladeJavaScript\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\BladeJavaScript\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\BladeJavaScript\\BladeJavaScriptServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
